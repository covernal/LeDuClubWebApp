{
  "name": "endorsse",
  "version": "0.1.0",
  "description": "LeDu Club Web App",
  "main": "server.js",
  "license": "MIT",
  "repository": "https://bitbucket.org/leduclub/leduclubwebapp",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js --require ignore-styles",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test && flow check ",
    "build": "cross-env --env=dist webpack src/index.js --config cfg/dist.js",
    "serve:local": "node server.js --env=local",
    "serve:dev": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "postinstall": "npm run clean && npm run build"
  },
  "author": {
    "name": "Vernal Liu",
    "email": "covernal@hotmail.com",
    "url": "localhost"
  },
  "devEngines": {
    "node": ">=5"
  },
